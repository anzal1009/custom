<odoo>
    <data>
        <record id="sequence_domestic_vendor_code" model="ir.sequence">
            <field name="name">Domestic Vendor Code</field>
            <field name="code">domestic.vendor.code</field>
            <field name="prefix">DV</field>
            <field name="padding" eval="5"/>
        </record>

        <record id="sequence_domestic_customer_code" model="ir.sequence">
            <field name="name">Domestic Customer Code</field>
            <field name="code">domestic.customer.code</field>
            <field name="prefix">DC</field>
            <field name="padding" eval="5"/>
        </record>

        <record id="sequence_import_export_vendor_code" model="ir.sequence">
            <field name="name">Import/Export Vendor Code</field>
            <field name="code">export.import.vendor.code</field>
            <field name="prefix">EIV</field>
            <field name="padding" eval="5"/>
        </record>

        <record id="sequence_import_export_customer_code" model="ir.sequence">
            <field name="name">Import/Export Customer Code</field>
            <field name="code">export.import.customer.code</field>
            <field name="prefix">EIC</field>
            <field name="padding" eval="5"/>
        </record>

        <record id="sequence_vendor_customer_code" model="ir.sequence">
            <field name="name">Vendor/ Customer Code</field>
            <field name="code">vendor.customer.code</field>
            <field name="prefix">VC</field>
            <field name="padding" eval="5"/>
        </record>
    </data>
</odoo>
<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="seq_medical_product" model="ir.sequence">
            <field name="name">Product Sequences</field>
            <field name="code">medical.products</field>
            <field name="prefix">MP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_medical_sales" model="ir.sequence">
            <field name="name">Sales Sequences</field>
            <field name="code">medical.sales</field>
            <field name="prefix">IN</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_medical_staff" model="ir.sequence">
            <field name="name">Staff Sequences</field>
            <field name="code">medical.staff</field>
            <field name="prefix">EMP</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
